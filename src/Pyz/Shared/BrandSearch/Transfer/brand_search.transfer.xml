<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="BrandSearch">
        <property name="idBrand" type="int"/>
        <property name="name" type="string"/>
        <property name="description" type="string"/>
        <property name="isHighlight" type="bool"/>
        <property name="isSearchable" type="bool"/>
        <property name="url" type="string"/>
        <property name="logo" type="string"/>
        <property name="metaDescription" type="string"/>
        <property name="productAbstractBrands" type="ProductBrandAbstract[]" singular="productAbstractBrands"/>
    </transfer>

    <transfer name="ProductBrandAbstract">
        <property name="idProductBrand" type="int"/>
        <property name="fkProductAbstract" type="int"/>
        <property name="fkBrand" type="int"/>
        <property name="ProductAbstract" type="ProductAbstract"/>
    </transfer>

</transfers>
